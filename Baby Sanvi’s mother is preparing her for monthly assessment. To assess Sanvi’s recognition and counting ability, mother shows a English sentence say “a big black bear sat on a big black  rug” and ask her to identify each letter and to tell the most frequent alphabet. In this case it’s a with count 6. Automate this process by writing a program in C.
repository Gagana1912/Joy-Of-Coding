#include <stdio.h>
#include <string.h>
int main()
{
  	char str[100], result;
  	int len;
  	int count= -1;

  	int freq[256] = {0};

  	printf("\n Please Enter any String :  ");
  	scanf("%s",str);

  	len = strlen(str);

  	for(int i=0;i<len;i++)
  	{
  		freq[str[i]]++;
	}

  	for(int i=0;i<len;i++)
  	{
		if(count < freq[str[i]])
		{
			count=freq[str[i]];
			result=str[i];
		}
	}
	printf("\n %c character occuring %d times in given string", result,count);
    return 0;
}

